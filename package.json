    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "express": "^4.21.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "nock": "^13.5.5",
    "pino-pretty": "^11.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/cache-manager": "^4.0.6",
