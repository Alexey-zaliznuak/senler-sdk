  },
  "license": "MIT",
  "keywords": [
    "sdk",
    "senler",
    "api",
    "vk",
    "marketing"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
